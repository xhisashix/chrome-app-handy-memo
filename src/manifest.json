{
  "manifest_version": 3,
  "name": "Handy Memo",
  "version": "1.0",
  "icons": {
    "16": "./img/icon/icon16.png",
    "48": "./img/icon/icon16.png",
    "128": "./img/icon/icon16.png"
  },
  "action": {
    "default_popup": "./html/popup.html"
  },
  "host_permissions": [
    "https://developer.chrome.com/*"
  ],
  "permissions": [
    "tabs",
    "storage"
  ]
}